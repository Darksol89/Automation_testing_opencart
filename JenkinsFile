properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("First step") {
            steps {
                sh "free -h"
            }
        }
        
        stage("Get GIT version") {
            steps {
                sh "git --version"
            }
        }
    }
}
